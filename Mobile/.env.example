# Environment Configuration for Titipin Mobile App
# Copy this file to .env and fill in the values

# ===========================================
# ENVIRONMENT
# ===========================================
# Options: dev, prod
APP_ENVIRONMENT=dev

# ===========================================
# API CONFIGURATION
# ===========================================
# Development API URLs
DEV_API_BASE_URL=http://192.168.1.18:3000/api
DEV_SOCKET_URL=http://192.168.1.18:3000

# Production API URLs
PROD_API_BASE_URL=https://api.titipin.com/api
PROD_SOCKET_URL=https://api.titipin.com

# ===========================================
# SUPABASE CONFIGURATION
# ===========================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key

# ===========================================
# FIREBASE CONFIGURATION (Optional)
# ===========================================
FIREBASE_PROJECT_ID=titipin-app
FIREBASE_ANDROID_APP_ID=1:xxxxx:android:xxxxx
FIREBASE_IOS_APP_ID=1:xxxxx:ios:xxxxx

# ===========================================
# GOOGLE MAPS (Optional)
# ===========================================
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# ===========================================
# REQUEST SETTINGS
# ===========================================
REQUEST_TIMEOUT_SECONDS=30
